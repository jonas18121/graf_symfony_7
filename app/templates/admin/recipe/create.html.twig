{% extends 'admin/admin_base.html.twig' %}

{% block title %}Creer une recette{% endblock %}

{% block body %}
    <h1>Creer une recette</h1>

    {{ form_start(form) }}
        <div class="d-flex">
            {{ form_row(form.title) }}
            {{ form_row(form.duration) }}
        </div>
        {{ form_row(form.slug) }}
        {{ form_row(form.category) }}
        {{ form_row(form.text) }}
    {{ form_end(form) }}
{% endblock %}
